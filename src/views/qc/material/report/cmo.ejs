<div class="row">
    <div class="col-12 attendance">
        <div class="card">
            <div class="card-body">
                <h5 class="card-title font-weight-bold mb-4">
                    INCOMING RAW MATERIAL REPORT (CMO) (QC003-F02-04)
                </h5>
                <div class="table-responsive p-2">
                    <form action="/qc/materialreport/cmo" method="POST" class="pr-3">

                        <table class="table table-bordered table-sm mb-4">
                            <tr>
                                <td><b>Specification Number</b></td>
                                <td colspan="3">
                                    <b>RM-6001</b>
                                </td>
                            </tr>
                            <tr>
                                <td><b>Material Code</b></td>
                                <td colspan="3">
                                    <b>CM</b>
                                </td>
                            </tr>

                            <tr>
                                <td><b>Plant Origin</b></td>
                                <td colspan="3">
                                    <b>Mentha Arvensis</b>
                                </td>
                            </tr>

                            <tr>
                                <td><b>Storage Condition</b></td>
                                <td colspan="3">
                                    <b>Storage Condition</b>
                                </td>
                            </tr>
                        </table>

                        <table class="table table-bordered table-sm mb-4">
                            <tr>
                                <td><b>Report No.</b></td>
                                <td>
                                    <input type="text" class="form-control" name="reportNo" placeholder="Report No">
                                </td>
                                <td><b>Date</b></td>
                                <td>
                                    <input type="date" class="form-control" name="date">
                                </td>
                            </tr>
                            <tr>
                                <td><b>Invoice No.</b></td>
                                <td>
                                    <input type="text" class="form-control" name="invoiceNo" placeholder="Invoice No">
                                </td>
                                <td><b>Invoice Date</b></td>
                                <td>
                                    <input type="date" class="form-control" name="invoiceDate">
                                </td>
                            </tr>

                            <tr>
                                <td><b>Supplier’s name</b></td>
                                <td>
                                    <input type="text" class="form-control" name="supplierName"
                                        placeholder="Supplier’s name">
                                </td>
                                <td><b>Identification No.</b></td>
                                <td>
                                    <input type="text" class="form-control" name="identificationNo"
                                        placeholder="Identification No.">
                                </td>
                            </tr>

                            <tr>
                                <td><b>Net Weight ( Kg ).</b></td>
                                <td>
                                    <input type="text" class="form-control" name="netWeight"
                                        placeholder="Net Weight ( Kg ).">
                                </td>
                                <td><b>Number of drums</b></td>
                                <td>
                                    <input type="text" class="form-control" name="noOfDrums"
                                        placeholder="Number of drums">
                                </td>
                            </tr>

                            <tr>
                                <td><b>Sample amount</b></td>
                                <td>
                                    <input type="text" class="form-control" name="sampleAmount"
                                        placeholder="Sample amount">
                                </td>
                                <td><b>Sampled by</b></td>
                                <td>
                                    <input type="text" class="form-control" name="sampledBy" placeholder="Sampled by">
                                </td>
                            </tr>

                            <tr>
                                <td colspan="4"><b>GC Parameters (QC058)</b></td>
                            </tr>

                            <tr>
                                <td><b><i>Method</i></b></td>
                                <td>
                                    <input type="text" class="form-control" name="method" placeholder="Method">
                                </td>
                                <td><b><i>Equipment ID</i></b></td>
                                <td>
                                    <input type="text" class="form-control" name="equipmentID"
                                        placeholder="Equipment ID">
                                </td>
                            </tr>

                            <tr>
                                <td><b>Parameters</b></td>
                                <td colspan="2">
                                    <b>Parameters Actual parameters</b>
                                </td>
                                <td><b>Desirable parameters</b></td>
                            </tr>

                            <tr>
                                <td><b>Injection volume</b></td>
                                <td colspan="2">
                                    <input type="text" class="form-control" name="injectionVolume"
                                        placeholder="Injection volume">
                                </td>
                                <td>
                                    1.0 µl
                                </td>
                            </tr>

                            <tr>
                                <td><b>Column Temperature</b></td>
                                <td colspan="2">
                                    <input type="text" class="form-control" name="columnTemperature"
                                        placeholder="Column Temperature">
                                </td>
                                <td>
                                    150 ˚C
                                </td>
                            </tr>
                            <tr>
                                <td><b>Injector temperature</b></td>
                                <td colspan="2">
                                    <input type="text" class="form-control" name="injectorTemperature"
                                        placeholder="Injection volume">
                                </td>
                                <td>
                                    230 oC
                                </td>
                            </tr>
                            <tr>
                                <td><b>Detector temperature</b></td>
                                <td colspan="2">
                                    <input type="text" class="form-control" name="detectorTemperature"
                                        placeholder="Detector temperature">
                                </td>
                                <td>
                                    240 ˚C
                                </td>
                            </tr>
                            <tr>
                                <td><b>Helium flow rate</b></td>
                                <td colspan="2">
                                    <input type="text" class="form-control" name="heliumFlowRate"
                                        placeholder="Helium flow rate">
                                </td>
                                <td>
                                    5.0 mL/min
                                </td>
                            </tr>
                            <tr>
                                <td><b>Split ratio</b></td>
                                <td colspan="2">
                                    <input type="text" class="form-control" name="splitRatio" placeholder="Split ratio">
                                </td>
                                <td>
                                    10:1
                                </td>
                            </tr>
                            <tr>
                                <td><b>Programming ratio</b></td>
                                <td colspan="2">
                                    <input type="text" class="form-control" name="programmingRatio"
                                        placeholder="Programming ratio">
                                </td>
                                <td>
                                    Isothermal
                                </td>
                            </tr>


                        </table>

                        <table class="table table-bordered table-sm mb-4">

                            <tr>
                                <td>
                                    <b>Sr. No.</b>
                                </td>
                                <td>
                                    <b>Test</b>
                                </td>
                                <td>
                                    <b>Test Result</b>
                                </td>
                                <td>
                                    <b>Acceptable Limits</b>
                                </td>
                            </tr>

                            <tr>
                                <td><b>1.</b></td>
                                <td colspan="3"><b>Physical Observations</b></td>

                            </tr>

                            <tr>
                                <td><b>1.1</b></td>
                                <td><b>Description</b></td>
                                <td>
                                    <input type="text" class="form-control" name="description"
                                        placeholder="Description">
                                </td>
                                <td>
                                    Slightly yellowish to yellowish coloured liquid
                                </td>
                            </tr>


                            <tr>
                                <td><b>1.2</b></td>
                                <td><b>Odour</b></td>
                                <td>
                                    <input type="text" class="form-control" name="odour"
                                        placeholder="Odour">
                                </td>
                                <td>
                                    Characteristic mint odour
                                </td>
                            </tr>


                            <tr>
                                <td><b>2.</b></td>
                                <td><b>Acidity or Alkalinity</b></td>
                                <td>
                                    <input type="text" class="form-control" name="acidity"
                                        placeholder="Acidity or Alkalinity">
                                </td>
                                <td>
                                    Neutral to Litmus
                                </td>
                            </tr>

                            <tr>
                                <td><b>3.</b></td>
                                <td class="d-flex py-2 justify-content-center align-items-center">
                                    <b>Specific Optical Rotation</b>
                                    <div class="dropdown">
                                        <p class="rounded px-1 mb-0 dropdown-toggle" style="font-size: 18px; font-weight: bold; cursor:pointer"
                                            id="dropdownMenuButton" data-toggle="dropdown" aria-expanded="false">
                                            <!-- + -->
                                        </p>
                                        <div class="dropdown-menu bg-warning" aria-labelledby="dropdownMenuButton">
                                            <a class="dropdown-item" href="/qc/oticalrotaion/rm">RM</a>
                                            <a class="dropdown-item" href="/qc/oticalrotaionwip">WIP</a>
                                            <a class="dropdown-item" href="/qc/oticalrotaion/sample">Sample</a>
                                        </div>
                                    </div>
                                </td>
                                <td>
                                    <input type="text" class="form-control" name="specificOpticalRotation"
                                        placeholder="Specific Optical Rotation">
                                </td>
                                <td>
                                    -18.0˚ to -37.0˚
                                </td>
                            </tr>

                            <tr>
                                <td><b>4.</b></td>
                                <td colspan="3"><b>Assay by GC</b></td>
                            </tr>
                            <tr>
                                <td><b>4.1</b></td>
                                <td><b>Total Terpenes</b></td>
                                <td>
                                    <input type="text" class="form-control" name="totalTerpenes"
                                        placeholder="Total Terpenes">
                                </td>
                                <td>
                                    NMT 9.0 %
                                </td>
                            </tr>


                            <tr>
                                <td><b>4.2</b></td>
                                <td><b>Menthone</b></td>
                                <td>
                                    <input type="text" class="form-control" name="menthone"
                                        placeholder="Menthone">
                                </td>
                                <td>
                                    5.0 – 10.0 %
                                </td>
                            </tr>

                            <tr>
                                <td><b>4.3</b></td>
                                <td><b>Menthyl Acetate</b></td>
                                <td>
                                    <input type="text" class="form-control" name="menthylAcetate"
                                        placeholder="Menthyl Acetate">
                                </td>
                                <td>
                                    NMT 6.0 %
                                </td>
                            </tr>

                            <tr>
                                <td><b>4.4</b></td>
                                <td><b>Neo Menthol</b></td>
                                <td>
                                    <input type="text" class="form-control" name="neoMenthol"
                                        placeholder="Neo Menthol">
                                </td>
                                <td>
                                    NMT 3.0 %
                                </td>
                            </tr>

                            <tr>
                                <td><b>4.5</b></td>
                                <td><b>L-Menthol</b></td>
                                <td>
                                    <input type="text" class="form-control" name="lMenthol"
                                        placeholder="L-Menthol">
                                </td>
                                <td>
                                    Not Less Than 66.0 %
                                </td>
                            </tr>

                        </table>

                        <table class="table table-bordered table-sm">

                            <tr>
                                <td colspan="6">
                                    <b>Report:</b>
                                </td>
                            </tr>

                            <tr>
                                <td colspan="6">
                                    The above lot sample of raw material is passed/not passed. It can be/cannot be used
                                    for further process.
                                </td>
                            </tr>

                            <tr>
                                <td colspan="6">
                                    <textarea type="text" name="report" class="form-control" row="3"></textarea>
                                </td>
                            </tr>

                            <tr>
                                <td>
                                    <b>Date:</b>
                                </td>
                                <td>
                                    <input type="date" class="form-control" name="reportDate">
                                </td>
                                <td>
                                    <b>Chemist – Quality Control </b>
                                </td>
                                <td>
                                    <input type="text" class="form-control" name="chemistQualityControl">
                                </td>
                                <td>
                                    <b>Head Quality</b>
                                </td>
                                <td>
                                    <input type="text" class="form-control" name="headQuality">
                                </td>
                            </tr>

                            <tr>
                                <td colspan="6">
                                    <button class="btn btn-warning px-5">Submit</button>
                                </td>
                            </tr>


                        </table>
                    </form>
                </div>
            </div>
        </div>

    </div>
</div>